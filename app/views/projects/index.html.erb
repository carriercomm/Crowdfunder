<% if !current_user %>
<%= link_to 'Login', new_session_path %> | <%= link_to 'Sign up', new_user_path %><br><br>
<% end %>
<% if current_user %>
	Logged in as <%= current_user.email %> | <%= link_to 'Log out', session_path(current_user), method: :delete %>
<% end %>
<h1>Projects#index</h1>
<p>Find me in app/views/projects/index.html.erb</p>
<%= link_to 'New', new_project_path %><br><br>

<% @projects.each do |x| %>
<strong><%= x.name %></strong>
<%= x.description %>
<%= x.id %>

<%= link_to 'Show', project_path(x.id) %>
<br>

<% end %>
